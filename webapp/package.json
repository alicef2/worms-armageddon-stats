{
  "name": "wormsabouts",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "concurrently": "^4.1.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "nodemon": "^1.19.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.15.2"
  },
  "devDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "concurrently -n \"Client,Server,Database\" \"yarn start-client\" \"yarn start-server\" \"yarn start-db\"",
    "start-client": "react-scripts start",
    "start-server": "nodemon --watch server/ server/index.js",
    "start-db": "./database/start_postgres.sh",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
